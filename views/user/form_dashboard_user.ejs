<meta charset="UTF-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta name="viewport" content="width=device-width, initial-scale=1.0">
<meta name="apple-mobile-web-app-title" content="CodePen">
<title>Dashboard</title>

<link rel="stylesheet" href="/asset/styless_dashboard.css">
<link rel="stylesheet" href="/asset/owl.carousel.min.css" type="text/css">
<link rel="stylesheet" href="/asset/owl.theme.default.css" type="text/css">

<!-- link data table -->   
<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
<link rel="stylesheet" href="https://cdn.datatables.net/1.10.24/css/dataTables.bootstrap.min.css">

<!--  -->
<link rel="apple-touch-icon" type="image/png" href="https://cpwebassets.codepen.io/assets/favicon/apple-touch-icon-5ae1a0698dcc2402e9712f7d01ed509a57814f994c660df9f7a952f3060705ee.png" />
<link rel="shortcut icon" type="image/x-icon" href="/image/Group 22.png" />
<link rel="mask-icon" type="" href="https://cpwebassets.codepen.io/assets/favicon/logo-pin-8f3771b1072e3c38bd662872f6b673a722f4b3ca2421637d5596661b4e2132cc.svg" color="#111" />
<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">  

</head>
<%- include('container_user'); -%>

  <div class="contents">
    <div class="isi-content">
      <div class="container">
        <div class="judul-content-1" id="judul-content-1">
          <h3>Dashboard</h3>
            <div class="content-1">
              <h1>Learn With Effectively With Us!</h1>
              <h2>Get 30% Off every course on January.</h2>
            </div>
        </div>

        <div class="content-2" id="content-2">
          <span>Aktivitas</span>
          <div class="isi-content-2">
            <ul>
              <li class="pers-cont">90%</li>
              <li class="ket-cont-2">Kehadiran</li>
              <li class="pers-cont">80%</li>
              <li class="ket-cont-2">Ketuntasan</li>
              <li class="pers-cont">50%</li>
              <li class="ket-cont-2">Quiz Minggu Ini</li>
              <li class="pers-cont">90%</li>
              <li class="ket-cont-2">Mata Pelajaran</li>
            </ul>
          </div>
        </div>

        <div class="content-3" id="content-3">
          <span>Pilih Kelas Anda : </span>
          <div class="isi-content-3">
            <form action="/pilih_pelajaran_user" method="POST">
            <div class="owl-carousel owl-theme" style="width: 100%; padding-top: 30px; text-align: center;">

              <%
                if(kelas.length != 0){
                kelas.forEach(function(data){
              %>
                <div class="item">
                  <button name="btn_namaKelas" type="submit" style="height: 80px; width: 200px;" value="<%= data.id_kelas%>">
                    <%= data.nama_kelas%>
                  </button>
                  <!-- <a href="/home/pilih_pelajaran_user" style="display: block;"><%= data.nama_kelas%></a> -->
                </div>
              <% }); %>
              <% }; %>
              
            </div>
          </form>
          </div>
        </div>
        
        <div class="content-4" id="content-4">
          <span>Daftar Quiz Minggu Ini</span>

          <div class="cont_table4">
            <table id="example" class="table table-striped table-bordered" style="width:100%">
              <thead>
                  <tr>
                      <th>Name</th>
                      <th>Position</th>
                      <th>Office</th>
                      <th>Age</th>
                      <th>Start date</th>
                      <th>Salary</th>
                  </tr>
              </thead>
              <tbody>
                  <tr>
                      <td>Tiger Nixon</td>
                      <td>System Architect</td>
                      <td>Edinburgh</td>
                      <td>61</td>
                      <td>2011/04/25</td>
                      <td>$320,800</td>
                  </tr>
              </tbody>
              <tfoot>
                  <tr>
                      <th>Name</th>
                      <th>Position</th>
                      <th>Office</th>
                      <th>Age</th>
                      <th>Start date</th>
                      <th>Salary</th>
                  </tr>
              </tfoot>
            </table>
          </div>
        </div>

        <div id="akhir-konten">
          <%- include('footer'); -%>
        </div>

      </div>
    </div>
  </div>

<script src="https://code.jquery.com/jquery-3.5.1.min.js"></script>
<script src="/js/owl.carousel.js" ></script>

<script>
    $('.owl-carousel').owlCarousel({
        // rtl:true,
        loop:false,
        margin:10,
        autoWidth: true,
        nav:true,
        responsive:{
            0:{
                items:1
            },
            600:{
                items:3
            },
            1000:{
                items:5
            }
        }
    })
</script>

<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
<script src="https://cdn.datatables.net/1.10.24/js/jquery.dataTables.min.js"></script>
<script src="https://cdn.datatables.net/1.10.24/js/dataTables.bootstrap.min.js"></script>

<script>
  function openNav() {
      document.getElementById("judul-content-1").style.marginLeft = "100px";
      document.getElementById("content-3").style.marginLeft = "100px";
      document.getElementById("content-2").style.marginLeft = "100px";
      document.getElementById("content-4").style.marginLeft = "100px";
      document.getElementById("judul-content-1").style.width = "91%";
      document.getElementById("akhir-konten").style.marginLeft = "100px";
      document.getElementById("akhir-konten").style.width = "91%";
  }

  function closeNav() {
      document.getElementById("judul-content-1").style.marginLeft = "0";
      document.getElementById("content-3").style.marginLeft = "0";
      document.getElementById("content-2").style.marginLeft = "0";
      document.getElementById("content-4").style.marginLeft = "0";
      document.getElementById("judul-content-1").style.width = "100%";
      document.getElementById("akhir-konten").style.marginLeft = "0";
      document.getElementById("akhir-konten").style.width = "100%";
  }
</script>

<script>
  $(document).ready(function() {
    $('#example').DataTable();
} );
</script>

</body>
</html>
 
